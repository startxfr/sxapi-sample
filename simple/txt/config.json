{
    "name": "sxapi-sample-simple-txt",
    "description": "sxapi simple sample text response (master). Display a static text response",
    "version": "master",
    "debug": true,
    "session": {},
    "plugins": [
    ],
    "log": {
        "filters": {
            "level": "0,1,2,3,4",
            "type": "debug,info,error,warn"
        }
    },
    "server": {
        "type": "express",
        "port": "19777",
        "bodyParserJson": false,
        "bodyParserUrl": true,
        "endpoints": [
            {
                "path": "/",
                "code": "201",
                "header": {
                    "Content-Type": "text/plain"
                },
                "body": "text response"
            }
        ]
    }
}
