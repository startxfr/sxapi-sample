{
    "name": "sxapi-sample-simple-hello",
    "description": "sxapi simple sample hello world response (v0.0.17). Display a static txt content",
    "version": "0.0.17",
    "server": {
        "endpoints": [
            {
                "path": "/",
                "header": {
                    "Content-Type": "text/plain"
                },
                "body": "hello world !!!"
            }
        ]
    }
}