{
    "name": "sxapi-sample-simple-hello",
    "description": "sxapi simple sample hello world response (0.0.6). Display a static txt content",
    "version": "0.0.6",
    "debug": true,
    "session": {},
    "plugins": [
    ],
    "log": {
        "filters": {
            "level": "0,1,2,3,4",
            "type": "debug,info,error,warn"
        }
    },
    "server": {
        "type": "express",
        "port": "8080",
        "bodyParserJson": false,
        "bodyParserUrl": true,
        "endpoints": [
            {
                "path": "/",
                "header": {
                    "Content-Type": "text/plain"
                },
                "body": "hello world !!!"
            }
        ]
    }
}
