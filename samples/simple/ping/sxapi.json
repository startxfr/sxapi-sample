{
    "name": "sxapi-sample-simple-ping",
    "description": "sxapi simple sample ping response (v0.0.7). Display a dynamic json response to a ping",
    "version": "0.0.7",
    "debug": true,
    "session": {},
    "plugins": [
    ],
    "log": {
        "filters": {
            "level": "0,1,2,3,4",
            "type": "debug,info,error,warn"
        }
    },
    "server": {
        "type": "express",
        "port": "8080",
        "bodyParserJson": true,
        "bodyParserUrl": true,
        "endpoints": [
            {
                "path": "/",
                "code": "200",
                "header": {
                    "Content-Type": "application/json"
                },
                "body": "{\"health\":\"good\"}"
            }
        ]
    }
}
