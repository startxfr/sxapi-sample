{
    "name": "sxapi-sample-simple-json",
    "description": "sxapi simple sample json response (dev). Display a static json response",
    "version": "dev",
    "debug": true,
    "session": {},
    "plugins": [
    ],
    "log": {
        "filters": {
            "level": "0,1,2,3,4",
            "type": "debug,info,error,warn"
        }
    },
    "server": {
        "type": "express",
        "port": "8080",
        "bodyParserJson": true,
        "bodyParserUrl": true,
        "endpoints": [
            {
                "path": "/json",
                "code": "200",
                "header": {
                    "Content-Type": "application/json"
                },
                "body": "{\"key\":\"value\"}"
            }
        ]
    }
}
