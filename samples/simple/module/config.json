{
    "name": "sxapi-sample-simple-module",
    "description": "sxapi simple sample response from a module (0.0.6). Display a dynamic json response",
    "version": "0.0.6",
    "debug": true,
    "session": {},
    "plugins": [
    ],
    "log": {
        "filters": {
            "level": "0,1,2,3,4",
            "type": "debug,info,error,warn"
        }
    },
    "server": {
        "type": "express",
        "port": "8080",
        "bodyParserJson": true,
        "bodyParserUrl": true,
        "endpoints": [
            {
                "path": "/function",
                "handler": "$ws.dynamicRequestHandlerTest",
                "param_sample": "value de param_sample"
            }
        ]
    }
}
